		<value>.*testEmdCfg_.*</value>
		<value>.*SDCfg2_.*</value>
		<value>.*_01_.*</value>
		<value>.*_02_.*</value>
		<value>.*_05_.*</value>		
	</util:list>	
	
	<util:list id="excludedClassifiersAndAverages">
		<value>.*_01_.*</value>
		<value>.*_02_.*</value>
		<value>.*_05_.*</value>
