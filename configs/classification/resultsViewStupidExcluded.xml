	<util:list id="baselinePattern">
	 			<bean class="testing.classificators.view.Value">
					<property name="value" value=".*baseline.*"/>
					<property name="filename" value="baseline"/>
				</bean>
	 			<bean class="testing.classificators.view.Value">
					<property name="value" value="(?!.*baseline.*).*"/>
					<property name="filename" value="not_baseline"/>
				</bean>
	</util:list>		
