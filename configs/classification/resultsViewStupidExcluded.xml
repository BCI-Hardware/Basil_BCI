<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:util="http://www.springframework.org/schema/util"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       
       xsi:schemaLocation="
                http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans.xsd
                http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<import resource="classifiers.xml"/>

	<bean id="ClassifiersSuccessViewer" class="testing.classificators.view.ClassifiersSuccessViewer">
		<property name="dataDir" value="c://hhtResults//classifiers//"/>
		<property name="patterns" ref="configsPatterns"/>	
		<property name="resultsDir" value="c://hhtResults//classifiers//configs" />
		<property name="next" ref="configsSum" />
		<property name="excludedPatterns" ref="excludedClassifiersAndAverages"/>
	</bean>
	
	<bean id="configsSum" class="testing.classificators.view.ClassifiersSuccessViewer">
		<property name="dataDir" value="c://hhtResults//classifiers//configs"/>
		<property name="resultsDir" value="c://hhtResults//classifiers//configs" />
		<property name="next" ref="classifiers" />
	</bean>

	<bean id="classifiers" class="testing.classificators.view.ClassifiersSuccessViewer">
		<property name="dataDir" value="c://hhtResults//classifiers//"/>
		<property name="resultsDir" value="c://hhtResults//classifiers//classifiers" />
		<property name="patterns" ref="classifiersPattern"/>
		<property name="next" ref="classifiersSum" />
		<property name="excludedPatterns" ref="excludedCfgsAndAverages"/>
	</bean>
	<bean id="classifiersSum" class="testing.classificators.view.ClassifiersSuccessViewer">
		<property name="dataDir" value="c://hhtResults//classifiers//classifiers"/>
		<property name="resultsDir" value="c://hhtResults//classifiers//classifiers" />
		<property name="next" ref="averages" />
	</bean>
	<bean id="averages" class="testing.classificators.view.ClassifiersSuccessViewer">
		<property name="dataDir" value="c://hhtResults//classifiers//"/>
		<property name="resultsDir" value="c://hhtResults//classifiers//averages" />
		<property name="patterns" ref="averagesPattern"/>
		<property name="next" ref="averagesSum" />
		<property name="excludedPatterns" ref="excludedClassifiersAndCfgs"/>
	</bean>
	<bean id="averagesSum" class="testing.classificators.view.ClassifiersSuccessViewer">
		<property name="dataDir" value="c://hhtResults//classifiers//averages"/>
		<property name="resultsDir" value="c://hhtResults//classifiers//averages" />
		<property name="next" ref="baseline" />
	</bean>
	
	<bean id="baseline" class="testing.classificators.view.ClassifiersSuccessViewer">
		<property name="dataDir" value="c://hhtResults//classifiers//"/>
		<property name="resultsDir" value="c://hhtResults//classifiers//baseline" />
		<property name="patterns" ref="baselinePattern"/>
		<property name="next" ref="baselineSum" />
		<property name="excludedPatterns" ref="excludedClassifiersAndCfgsAndAverages"/>
	</bean>
	<bean id="baselineSum" class="testing.classificators.view.ClassifiersSuccessViewer">
		<property name="dataDir" value="c://hhtResults//classifiers//baseline"/>
		<property name="resultsDir" value="c://hhtResults//classifiers//baseline" />
	</bean>
	
	<util:list id="excludedClassifiersAndCfgsAndAverages">
		<value>.*FreqAmpl2.*</value>
